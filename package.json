{
  "name": "winter-love-server-se",
  "version": "0.0.1",
  "description": "A quick way to start hapi server with typescriptp",
  "author": "bichi <bichi@live.co.kr> (https://github.com/bichikim)",
  "main": "dist/app.js",
  "types": "scr/types.d.ts",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_END=development npm-run-all -p build:dev start:dev",
    "start": "pm2 start config/ecosystem.config.js",
    "start:watch": "pm2 start config/ecosystem.config.js --watch",
    "start:prod": "node dist/app.js",
    "start:dev": "nodemon --watch dist/app.js dist/app.js",
    "stop": "pm2 stop config/ecosystem.config.js",
    "restart": "pm2 restart config/ecosystem.config.js",
    "reload": "pm2 reload config/ecosystem.config.js",
    "delete": "pm2 delete config/ecosystem.config.js",
    "processes": "pm2 list config/ecosystem.config.js",
    "build": "yarn run build:prod",
    "build:prod": "babel-node build",
    "build:dev": "nodemon --watch .env.js --watch build --watch config build/dev",
    "doc": "typedoc -out ./docs ./src --media ./media",
    "deploy": "??",
    "lint": "yarn run lint:eslint && yarn run lint:tslint",
    "lint:tslint": "tslint --fix src/**/*.ts",
    "lint:eslint": "eslint --fix src/**/*.js src/**/*.ts",
    "test": "yarn run test:unit",
    "test:unit:watch": "yarn run test:unit --watch",
    "test:unit": "mocha-webpack \"src/**/*.spec.js\" --webpack-config config/webpack.test.config.js --timeout 3000 --recursive --require build/test.js",
    "cover": "nyc yarn run test:unit",
    "cover:watch": "nodemon --exec nyc yarn run test:unit",
    "cover:ci": "nyc --reporter=lcov yarn run test:unit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bichikim/winter-love-server-se.git"
  },
  "keywords": [
    "typescript"
  ],
  "bugs": {
    "url": "https://github.com/bichikim/winter-love-server-se/issues"
  },
  "homepage": "https://github.com/bichikim/winter-love-server-se#readme",
  "engines": {
    "node": ">=8.11",
    "npm": "~6"
  },
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/graphql": "^0.13.1",
    "@types/hapi": "^17.0.12",
    "@types/inert": "^5.1.1",
    "@types/joigoose": "^2.0.1",
    "@types/lodash": "^4.14.109",
    "@types/lowdb": "^1.0.2",
    "@types/minimist": "^1.2.0",
    "@types/mocha": "^5.2.1",
    "@types/mongoose": "^5.0.15",
    "@types/node": "^10.3.2",
    "@types/sinon": "^5.0.1",
    "@types/vision": "^5.3.4",
    "@types/webpack": "^4.4.0",
    "@types/webpack-env": "^1.13.6",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-lodash": "^3.3.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-1": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "chai": "^4.1.2",
    "codecov": "^3.0.2",
    "cross-env": "^5.1.6",
    "eslint": "^4.19.1",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-html": "^4.0.3",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-typescript": "^0.12.0",
    "eslint-plugin-vue": "^4.5.0",
    "esm": "^3.0.50",
    "istanbul-instrumenter-loader": "^3.0.1",
    "mocha": "^5.2.0",
    "mocha-typescript": "^1.1.15",
    "mocha-webpack": "^2.0.0-beta.0",
    "nodemon": "^1.17.5",
    "npm-run-all": "^4.1.3",
    "nyc": "^12.0.2",
    "sinon": "^5.1.1",
    "ts-loader": "^4.4.1",
    "ts-node": "^6.1.0",
    "tslint": "^5.10.0",
    "typedoc": "^0.11.1",
    "typescript": "^2.9.1",
    "typescript-eslint-parser": "^16.0.0",
    "webpack": "^4.12.0",
    "webpack-graphql-loader": "^0.1.2",
    "webpack-merge": "^4.1.2",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "apollo-server-hapi": "^1.3.6",
    "bell": "^9.3.1",
    "boom": "^7.2.0",
    "good": "^8.1.1",
    "good-console": "^7.1.0",
    "good-loggly": "^3.1.0",
    "good-squeeze": "^5.1.0",
    "graphql": "^0.13.2",
    "graphql-tools": "^3.0.2",
    "hapi": "^17.5.1",
    "hapi-auth-jwt2": "^8.1.0",
    "hapi-pino": "^4.0.4",
    "hapi-swagger": "^9.1.1",
    "inert": "^5.1.0",
    "joi": "^13.3.0",
    "joigoose": "^4.0.0",
    "lodash": "^4.17.10",
    "lowdb": "^1.0.0",
    "minimist": "^1.2.0",
    "mongoose": "^5.1.4",
    "mongoose-schema-to-graphql": "^2.7.2",
    "pm2": "^2.10.4",
    "vision": "^5.3.3"
  }
}
