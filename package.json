{
  "name": "winter-love-server-se",
  "version": "0.0.1",
  "description": "A quick way to start hapi server with typescriptp",
  "author": "bichi <bichi@live.co.kr> (https://github.com/bichikim)",
  "main": "dist/app.js",
  "types": "scr/types.d.ts",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_END=development npm-run-all -p build:dev start:dev",
    "start": "pm2 start config/ecosystem.config.js",
    "start:watch": "pm2 start config/ecosystem.config.js --watch",
    "start:dev": "nodemon dist/app.js",
    "stop": "pm2 stop config/ecosystem.config.js",
    "restart": "pm2 restart config/ecosystem.config.js",
    "reload": "pm2 reload config/ecosystem.config.js",
    "delete": "pm2 delete config/ecosystem.config.js",
    "processes": "pm2 list config/ecosystem.config.js",
    "build": "babel-node build",
    "build:dev": "babel-node build/dev",
    "doc": "typedoc -out ./docs ./src --media ./media",
    "deploy": "??",
    "test": "yarn run test:unit",
    "test:unit:watch": "yarn run test-ci:unit --watch",
    "test:unit": "mocha-webpack \"src/**/*.spec.js\" --webpack-config config/webpack.test.config.js --timeout 3000 --recursive --require build/chai.js",
    "cover": "nyc --reporter=lcov --reporter=text yarn run test:unit"
  },
  "nyc": {
    "include": [
      "src/**/*.js",
      "src/**/*.ts"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "report-dir": "./.coverage",
    "instrument": false,
    "sourceMap": false
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bichikim/winter-love-server-se.git"
  },
  "keywords": [
    "typescript"
  ],
  "bugs": {
    "url": "https://github.com/bichikim/winter-love-server-se/issues"
  },
  "homepage": "https://github.com/bichikim/winter-love-server-se#readme",
  "engines": {
    "node": ">=8.11",
    "npm": "~6"
  },
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/hapi": "^17.0.12",
    "@types/inert": "^5.1.1",
    "@types/joigoose": "^2.0.1",
    "@types/lodash": "^4.14.109",
    "@types/lowdb": "^1.0.2",
    "@types/minimist": "^1.2.0",
    "@types/mocha": "^5.2.1",
    "@types/mongoose": "^5.0.15",
    "@types/node": "^10.3.2",
    "@types/sinon": "^5.0.1",
    "@types/vision": "^5.3.4",
    "@types/webpack": "^4.4.0",
    "@types/webpack-env": "^1.13.6",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-lodash": "^3.3.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-1": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "chai": "^4.1.2",
    "codecov": "^3.0.2",
    "eslint": "^4.19.1",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-html": "^4.0.3",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-typescript": "^0.12.0",
    "eslint-plugin-vue": "^4.5.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "karma": "^2.0.2",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-coveralls": "^1.2.1",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-remap-coverage": "^0.1.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^3.0.0",
    "mocha": "^5.2.0",
    "mocha-typescript": "^1.1.15",
    "mocha-webpack": "^2.0.0-beta.0",
    "nodemon": "^1.17.5",
    "npm-run-all": "^4.1.3",
    "nyc": "^12.0.2",
    "sinon": "^5.1.1",
    "ts-loader": "^4.4.1",
    "ts-node": "^6.1.0",
    "tslint": "^5.10.0",
    "typedoc": "^0.11.1",
    "typescript": "^2.9.1",
    "typescript-eslint-parser": "^16.0.0",
    "webpack": "^4.12.0",
    "webpack-merge": "^4.1.2",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "apollo-server-hapi": "^1.3.6",
    "bell": "^9.3.1",
    "boom": "^7.2.0",
    "cross-env": "^5.1.6",
    "graphql": "^0.13.2",
    "hapi": "^17.5.1",
    "hapi-auth-jwt2": "^8.1.0",
    "hapi-swagger": "^9.1.1",
    "inert": "^5.1.0",
    "joi": "^13.4.0",
    "joigoose": "^4.0.0",
    "lodash": "^4.17.10",
    "lowdb": "^1.0.0",
    "minimist": "^1.2.0",
    "mongoose": "^5.1.4",
    "pm2": "^2.10.4",
    "vision": "^5.3.3"
  }
}
